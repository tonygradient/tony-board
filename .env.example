# Database
DATABASE_URL=postgresql://jarvis:jarvis@localhost:5432/jarvis

# Web UI Authentication (username/password + cookie session)
# Leave AUTH_PASS empty to disable UI authentication
AUTH_USER=admin
AUTH_PASS=your-secure-password

# API Authentication (Bearer tokens)
# Comma-separated list of valid tokens for API access
# Generate: openssl rand -hex 32
# Leave empty to disable API authentication
API_TOKENS=your-api-token-here
